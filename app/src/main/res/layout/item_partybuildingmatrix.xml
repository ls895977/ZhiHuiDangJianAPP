<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/dip_5"
    android:background="@color/white"
    android:orientation="vertical">
    <ImageView
        android:layout_width="@dimen/dip_60"
        android:layout_gravity="center"
        android:layout_height="@dimen/dip_60"
        android:src="@mipmap/icon_dangjian"/>
    <TextView
        android:id="@+id/item_name"
        android:layout_width="wrap_content"
        android:paddingTop="@dimen/dip_5"
        android:paddingLeft="@dimen/dip_5"
        android:paddingRight="@dimen/dip_5"
        android:textSize="@dimen/fond_12sp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="中共山西省中条山国有林管理局委员会" />
</LinearLayout>
